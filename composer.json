{
    "name": "serge0design/pimcore-starterkit",
    "license": "GPL-3.0-or-later",
    "type": "pimcore-bundle",
    "keywords": [
        "pimcore",
        "starterkit"
    ],
    "description": "This Pimcore StarterkitBundle copies some recommended files to setup a boilerplate template.",
    "homepage": "https://github.com/serge0design/pimcore-starterkit.git",
    "authors": [
        {
            "name": "serge.design - Serge",
            "email": "github@serge.design",
            "homepage": "https://serge.design",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "StarterkitBundle\\": "src/StarterkitBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "src/"
        }
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "StarterkitBundle\\StarterkitBundle"
            ]
        }
    },
    "require": {
        "pimcore/pimcore": "10.*",
        "serge0design/pimcore-starterkit-areas": "^1.0",
        "symfony/webpack-encore-bundle" : "^v1.16",
        "dachcom-digital/formbuilder": "^4.2"
    },
    "require-dev": {}
}
